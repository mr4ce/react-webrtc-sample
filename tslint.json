{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-microsoft-contrib/latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "linterOptions": {
    "exclude": [
      "src/generated/*"
    ]
  },
  "rules": {
    "export-name": [
      false
    ],
    "function-name": false,
    "no-relative-imports": [
      false
    ],
    "no-suspicious-comment": false,
    "object-literal-sort-keys": [
      false
    ],
    "prefer-array-literal": false,
    "prettier": true,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ]
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
